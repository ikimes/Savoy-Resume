<body style="background-color:#F7F7F7; overflow:visible">
   
   <div class="container" style="margin-top:5%">
       <div class="row">
      
                 <div class="col-sm-12 col-md-12 col-lg-12" post-body>
                 <div class="post">
                  <div class="post-img-content">
                           <img class="img-responsive" width="100%" src="<%= data.image %>">
                           <span class="post-title"><b><%= data.title %></b></span>
                     </div>
                   </div>
                   <div class="content" style="padding:3%;background-color:#fff">
                                
                                
                                <h1 style="text-align:center;padding:2%"><%= data.subtitle %></h1>
                                
                                <p style="padding:2%"><%= data.body %></p>
                            </div>
           </div>
       </div>
   </div>
   
   
   
